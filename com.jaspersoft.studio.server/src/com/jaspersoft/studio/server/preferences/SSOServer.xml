<mapping>
	<class name="com.jaspersoft.studio.server.preferences.SSOServer">
		<map-to xml="SSOServer" />

		<field name="uuid" type="string">
			<bind-xml name="uuid" node="element" />
		</field>
		<field name="url" type="string">
			<bind-xml name="url" node="element" />
		</field>
		<field name="user" type="string">
			<bind-xml name="user" node="element" />
		</field>
		<field name="password" type="string">
			<bind-xml name="password" node="element" />
		</field>

		<field name="type" type="com.jaspersoft.studio.server.preferences.SSOTypes">
			<bind-xml name="type" node="element" />
		</field>
	</class>
</mapping>

