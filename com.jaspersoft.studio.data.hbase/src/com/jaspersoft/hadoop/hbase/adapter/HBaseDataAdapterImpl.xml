<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class
		name="com.jaspersoft.hadoop.hbase.adapter.HBaseDataAdapterImpl">
		<map-to xml="HBaseDataAdapter" />
		<field name="class" type="java.lang.Class"
			handler="net.sf.jasperreports.data.ClassNameFieldHandler">
			<bind-xml name="class" node="attribute" />
		</field>
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="zookeeperQuorum" type="string">
			<bind-xml name="zookeeperQuorum" node="element" />
		</field>
		<field name="zookeeperClientPort" type="string">
			<bind-xml name="zookeeperClientPort" node="element" />
		</field>
	</class>
</mapping>
